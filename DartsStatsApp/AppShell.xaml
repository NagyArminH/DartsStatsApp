<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DartsStatsApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DartsStatsApp"
    xmlns:views="clr-namespace:DartsStatsApp.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="DartsStatsApp">

    <TabBar>
        <Tab Title="OOM List">
            <ShellContent ContentTemplate="{DataTemplate views:OOMListView}"/>
        </Tab>
        <Tab Title="OOM List">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}"/>
        </Tab>
    </TabBar>

</Shell>
